{
  "name": "blog",
  "private": true,
  "version": "1.5.2",
  "scripts": {
    "prepare": "husky install",
    "dev": "rsbuild dev --open",
    "posts:build": "node ./scripts/build-posts.js",
    "build": "npm run posts:build && rsbuild build",
    "preview": "rsbuild preview",
    "release": "standard-version"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.65.0",
    "highlight.js": "^11.11.1",
    "inquirer": "8.2.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.3"
  },
  "devDependencies": {
    "@arco-plugins/unplugin-react": "^1.0.2",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@rsbuild/core": "^1.1.8",
    "@rsbuild/plugin-less": "^1.1.0",
    "@rsbuild/plugin-react": "^1.0.7",
    "@types/node": "^22.10.8",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gray-matter": "^4.0.3",
    "husky": "7.0.1",
    "lint-staged": "^15.4.1",
    "marked": "^12.0.0",
    "mkdirp": "^3.0.1",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "typescript": "5.7.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,mjsx,cjs,cjsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js -c .eslintrc.js --fix --cache --quiet"
    ]
  }
}
